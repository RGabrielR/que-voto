// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model guess_apartment {
  id            Int     @id @default(autoincrement())
  description   String
  location      String
  prizeInARS    String
  prizeInUSD    String?
  meters        String
  arrayOfImages Json
}
